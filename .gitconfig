[remote "origin"]
    url = git@github.com:gnachman/iTerm2.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/iTerm2.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[branch "master.fix"]
    remote = github
    merge = refs/heads/master.fix
[commit]
	message = n
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/mac/gnachman/iTerm2
[gdif]
    ignore = Scripts/AutoLaunch/move_current_tab.py 
    ignore = build.sh
    ignore = deploy.sh
    ignore = error
    ignore = movescreen.py
    ignore = readme
